<h1>New Page</h1>
<p>Find me in app/views/pages/new.html.erb</p>
<%= form_for @page do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br />
  <%= f.label :author %>
  <%= f.text_field :author %>
  <br />
  <%= f.label :body %>
  <%= f.text_area :body %>
  <br />
  <%= f.submit %>
<%= end %>


